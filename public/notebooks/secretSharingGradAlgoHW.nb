Cell["Generalized Secret Splitting", "Section",
 CellChangeTimes->{{3.6343981609513197`*^9, 3.634398167149228*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "To split the secret M among m people, choose m-1 random numbers from among \
0,1,\[Ellipsis],n-1: call the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "1"], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "2"], TraditionalForm]]],
 ",\[Ellipsis],",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", 
    RowBox[{"m", "-", "1"}]], TraditionalForm]]],
 "."
}], "Item",
 CellChangeTimes->{{3.634398177510303*^9, 3.6343982792304907`*^9}}],

Cell[TextData[{
 "Give the numbers ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "1"], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "2"], TraditionalForm]]],
 ",\[Ellipsis],",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", 
    RowBox[{"m", "-", "1"}]], TraditionalForm]]],
 " and M-(",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "1"], TraditionalForm]]],
 "+ ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "2"], TraditionalForm]]],
 "+\[Ellipsis]+",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", 
    RowBox[{"m", "-", "1"}]], TraditionalForm]]],
 ") to the m participants."
}], "Item",
 CellChangeTimes->{{3.634398177510303*^9, 3.634398332693069*^9}}],

Cell["\<\
Once they get together, they can determine the message M by computing\
\>", "Item",
 CellChangeTimes->{{3.634398177510303*^9, 3.6343983553080587`*^9}}]
}, Open  ]],

