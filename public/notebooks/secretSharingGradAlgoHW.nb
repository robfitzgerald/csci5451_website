Cell[CellGroupData[{

Cell["Shamir\[CloseCurlyQuote]s Threshold Scheme, continued", "Section",
 CellChangeTimes->{{3.634480733876582*^9, 3.634480737543289*^9}}],

Cell["The particulars:", "Text",
 CellChangeTimes->{{3.634398875673945*^9, 3.634398882969821*^9}}],

Cell[CellGroupData[{

Cell["Choose prime p larger than any possible message M.", "Item",
 CellChangeTimes->{{3.634398809030974*^9, 3.6343989076804934`*^9}}],

Cell["The message is represented as M (mod p).", "Item",
 CellChangeTimes->{{3.634398809030974*^9, 3.6343989267019157`*^9}}],

Cell["\<\
We wish to split message M among \[Omega] people so that any t among them can \
reconstruct M.\
\>", "Item",
 CellChangeTimes->{{3.634398809030974*^9, 3.634398949830633*^9}, {
  3.634480742745719*^9, 3.634480742754081*^9}}],

Cell["Randomly select t-1 integers (mod p).", "Item",
 CellChangeTimes->{{3.634398809030974*^9, 3.634398949830633*^9}, {
  3.634480742745719*^9, 3.6344807947893744`*^9}}],

Cell[TextData[{
 "Call them ",
 Cell[BoxData[
  FormBox[
   RowBox[{
